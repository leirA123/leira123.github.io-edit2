			<script type="text/javascript">
				function getAcoes(){
					var abas = [];
					var maioresBaixas = [];
					var maioresAltas = [];
					var maisNegociadas = [];
					$("#divInfoPregao").css("display","none");		
					
					$.ajax({
							url: "https://cotacao.b3.com.br/mds/api/v1/InstrumentPriceFluctuation/ibov",
							async: false
						}).done(function(data) {
							for(var x in data.SctyHghstDrpLst)
								maioresBaixas.push({"s":"BMFBOVESPA:"+data.SctyHghstDrpLst[x].symb, "d":data.SctyHghstDrpLst[x].desc});
							for(var x in data.SctyHghstIncrLst)
								maioresAltas.push({"s":"BMFBOVESPA:"+data.SctyHghstIncrLst[x].symb, "d":data.SctyHghstIncrLst[x].desc});
						});
					$.ajax({
							url: "https://cotacao.b3.com.br/mds/api/v1/InstrumentTradeVolume/vista",
							async: false
						}).done(function(data) {
							for(var x in data.Volume)
								maisNegociadas.push({"s":"BMFBOVESPA:"+data.Volume[x].scty.symb, "d":data.Volume[x].scty.desc});
						});
					abas.push({"title":"Maiores altas", "symbols": maioresAltas});
					abas.push({"title":"Maiores baixas", "symbols": maioresBaixas});
					abas.push({"title":"Mais negociadas", "symbols": maisNegociadas});
					return abas;
				}
				
				var returnAcoes = getAcoes();
				
				if (returnAcoes != null && returnAcoes[0].symbols.length > 0)
				{
					$("#divGraficoMaiores").css("display","block");				
					var widgetSettings = {
									  "showChart": false,
									  "locale": get_lang() == 'pt' ? 'br' : get_lang(),
									  "width": "100%",
									  "height": "400",
									  "customer": "bovespa",
									  "largeChartUrl": window.location.href.substring(0, window.location.href.lastIndexOf('/') + 1),
									  "tabs": returnAcoes
									};
					
					script = document.createElement('script');
					script.setAttribute('src', 'https://s3.tradingview.com/external-embedding/embed-widget-market-overview.js');
					script.setAttribute('async', "");						
					script.innerHTML = JSON.stringify(widgetSettings);
					document.getElementById("tradingview-widget-acoes").appendChild(script);
				}
				else
				{
					$("#divInfoPregao").css("display","block");		
					$("#divGraficoMaiores").css("display","none");
				}
			</script>			
        </div>	
		
        <div class="large-4 columns">
            <div class="panel callout">
                    <div class="list-avatar two-line">
