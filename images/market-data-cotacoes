                        </div>
                        <div class="content">
                          <p class="primary-text">
                            <span id="x-status-pregao">Pregão em andamento</span>
                          </p>
                          <p class="secondary-text">
                            <span id="x-data-pregao">-</span>
                          </p>
				<script type="text/javascript">
					  $.ajax({
					url: 'https://cotacao.b3.com.br/mds/api/v1/TradingFloorInfo'
				}).then(function(data) {
					var pregaoData = new Date(data.Msg.dtTm.replace(/[a-z|A-Z]+/g," ").trim().split("-").join("/"));
					$("#x-data-pregao").text(pregaoData.toLocaleString('pt-BR'));  
					var strFase = data.TradgFlr.TradgFlrSts.desc;
					if (strFase === "E")
						$("#x-status-pregao").text("Pregão encerrado");
					else
						$("#x-status-pregao").text("Pregão em andamento");
					var vol = data.TradgFlr.grssAmt !== undefined ? "R$ " + parseInt(data.TradgFlr.grssAmt).toLocaleString('pt-BR') : "---";
					var neg = data.TradgFlr.qty !== undefined ? parseInt(data.TradgFlr.qty).toLocaleString('pt-BR') : "---";
					$("#x-volume-pregao").text(vol);
					$("#x-qtd-pregao").text(neg);
				});
				</script>							
